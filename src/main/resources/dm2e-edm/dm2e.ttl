    foaf:name "Julia Iwanowa";
    foaf:homepage <> .

<http://dm2edata.hu-berlin.de/neologism/dm2e#School%20for%20Library%20and%20Information%20Science> a dcterms:Agent;
    foaf:member dm2e:JIwanowa;
    foaf:name "School for Library and Information Science";
    foaf:homepage <ww.ibi.hu-berlin.de> .

dm2e:Evelyn a foaf:Person;
    foaf:name "Evelyn Dr√∂ge";
    foaf:homepage <> .

<http://dm2edata.hu-berlin.de/neologism/dm2e#s.hennicke> a foaf:Person;
    foaf:name "Steffen Hennicke";
    foaf:homepage <> .

dm2e:IBI a dcterms:Agent;
    foaf:member <http://dm2edata.hu-berlin.de/neologism/dm2e#s.hennicke>;
    foaf:name "IBI";
    foaf:homepage <> .

dm2e:Version a rdfs:Class, owl:Class;
    rdfs:label "Version";
    rdfs:comment """The class Version can be used to save creation date and time of the 
      aggregation of the CHO.""";
    rdfs:subClassOf rdfs:Resource .

dm2e:Document a rdfs:Class, owl:Class;
    rdfs:label "Document";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type document." .

dm2e:Book a rdfs:Class, owl:Class;
    rdfs:label "Book";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type book." .

dm2e:Museum a rdfs:Class, owl:Class;
    rdfs:label "Museum";
    rdfs:comment "Subclass of dm2e:Institution.";
    rdfs:subClassOf dm2e:Institution .

dm2e:Photo a rdfs:Class, owl:Class;
    rdfs:label "Photo";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type photo." .

dm2e:Library a rdfs:Class, owl:Class;
    rdfs:label "Library";
    rdfs:comment "Subclass of dm2e:Institution.";
    rdfs:subClassOf dm2e:Institution .

dm2e:File a rdfs:Class, owl:Class;
    rdfs:label "File";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type file." .

dm2e:Person a rdfs:Class, owl:Class;
    rdfs:label "Person";
    rdfs:comment "Subclass of edm:Agent." .

dm2e:Institution a rdfs:Class, owl:Class;
    rdfs:label "Institution";
    rdfs:comment """The class Institution is a subclass of edm:Agent and describes the 
      institutions asn agents more detailed.""" .

dm2e:Archive a rdfs:Class, owl:Class;
    rdfs:label "Archive";
    rdfs:comment "Subclass of dm2e:Institution.";
    rdfs:subClassOf dm2e:Institution .

dm2e:Page a rdfs:Class, owl:Class;
    rdfs:label "Page";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type page. Can be part of another CHO, e.g. dm2e:Manuscript." .

dm2e:Letter a rdfs:Class, owl:Class;
    rdfs:label "Letter";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type letter. Has a creator and a recipient." .

dm2e:University a rdfs:Class, owl:Class;
    rdfs:label "University";
    rdfs:comment "Subclass of dm2e:Institution.";
    rdfs:subClassOf dm2e:Institution .

dm2e:Manuscript a rdfs:Class, owl:Class;
    rdfs:label "Manuscript";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type manuscript, e.g. Wittgensteins brown book." .

dm2e:Paragraph a rdfs:Class, owl:Class;
    rdfs:label "Paragraph";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type paragraph. Can be part of another (textual) CHO." .

dm2e:Work a rdfs:Class, owl:Class;
    rdfs:label "Work";
    rdfs:comment "A non-physical piece of work of an agent, e.g. the theoretical concept of a manuscript." .

dm2e:Publication a rdfs:Class, owl:Class;
    rdfs:label "Publication";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type publication." .

dm2e:Cover a rdfs:Class, owl:Class;
    rdfs:label "Cover";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type cover. Can be part of another CHO, e.g. a book." .

dm2e:Codex a rdfs:Class, owl:Class;
    rdfs:label "Codex";
    rdfs:comment "Subclass of edm:PhysicalThing. ProvidedCHO from type codex." .

dm2e:shelfmark a rdf:Property, owl:DatatypeProperty;
    rdfs:label "shelfmark";
    rdfs:comment "Shelfmark location from the ProvidedCHO";
    rdfs:range rdfs:Literal .

dm2e:condition a rdf:Property, owl:DatatypeProperty;
    rdfs:label "condition";
    rdfs:comment "Description of the CHOs condition.";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dc:description .

dm2e:writtenAreaDimension a rdf:Property, owl:DatatypeProperty;
    rdfs:label "writtenAreaDimension";
    rdfs:comment """Size of writing or the part of the page where something is actually 
      written. Please do also note the unit that was used!""";
    rdfs:range rdfs:Literal .

dm2e:pageDimension a rdf:Property, owl:DatatypeProperty;
    rdfs:label "pageDimension";
    rdfs:comment "Page size. Please do also note the unit that was used!";
    rdfs:range rdfs:Literal .

dm2e:watermark a rdf:Property, owl:DatatypeProperty;
    rdfs:label "watermark";
    rdfs:comment "Description of the CHOs watermark.";
    rdfs:range rdfs:Literal .

dm2e:subtitleTransliteration a rdf:Property, owl:DatatypeProperty;
    rdfs:label "subtitleTransliteration";
    rdfs:comment "Subtitle transliteration";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dm2e:subtitle .

dm2e:subtitle a rdf:Property, owl:DatatypeProperty;
    rdfs:label "subtitle";
    rdfs:comment "Any form of subtitle";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dc:title .

dm2e:support a rdf:Property, owl:DatatypeProperty;
    rdfs:label "support";
    rdfs:comment "Description of the CHOs support.";
    rdfs:range rdfs:Literal .

dm2e:referencedEdition a rdf:Property, owl:DatatypeProperty;
    rdfs:label "referencedEdition";
    rdfs:comment "References of Edition.";
    rdfs:range rdfs:Literal .

dm2e:titleTransliteration a rdf:Property, owl:DatatypeProperty;
    rdfs:label "titleTransliteration";
    rdfs:comment "Title transliteration";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dc:title .

dm2e:restoration a rdf:Property, owl:DatatypeProperty;
    rdfs:label "restoration";
    rdfs:comment "Description of the CHOs restoration.";
    rdfs:range rdfs:Literal .

dm2e:colophon a rdf:Property, owl:DatatypeProperty;
    rdfs:label "colophon";
    rdfs:comment "A brief description of the manuscript";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dc:description .

dm2e:referencedPublication a rdf:Property, owl:DatatypeProperty;
    rdfs:label "referencedPublication";
    rdfs:comment "Reference to publication.";
    rdfs:range rdfs:Literal .

dm2e:levelOfGenesis a rdf:Property;
    rdfs:label "levelOfGenesis";
    rdfs:comment "Level of Genesis.";
    rdfs:subPropertyOf dcterms:extent .

dm2e:relatedPublication a rdf:Property, owl:DatatypeProperty;
    rdfs:label "relatedPublication";
    rdfs:comment "Other related publications.";
    rdfs:range rdfs:Literal .

dm2e:incipit a rdf:Property, owl:DatatypeProperty, owl:FunctionalProperty;
    rdfs:label "incipit";
    rdfs:comment "Opening words of a manuscript.";
    rdfs:range rdfs:Literal .

dm2e:isbn a rdf:Property, owl:DatatypeProperty, owl:FunctionalProperty;
    rdfs:label "isbn";
    rdfs:comment "The ISBN number for the CHO";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dc:identifier .

dm2e:callNumber a rdf:Property, owl:DatatypeProperty, owl:FunctionalProperty;
    rdfs:label "callNumber";
    rdfs:comment "The call number for some archival item";
    rdfs:range rdfs:Literal;
    rdfs:subPropertyOf dc:identifier .

dm2e:explicit a rdf:Property, owl:DatatypeProperty, owl:FunctionalProperty;
    rdfs:label "explicit";
    rdfs:comment "Final words of a manuscript.";
    rdfs:range rdfs:Literal .

dm2e:dateOfRemark a rdf:Property;
    rdfs:label "dateOfRemark";
    rdfs:comment "Date of remark. can be point in time or a timespan!" .

dm2e:printer a rdf:Property;
    rdfs:label "printer";
    rdfs:comment "A person or institution who printed the CHO.  In the manuscript context especially used in works of the 16th and 17th century (rare books).";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:contributor a rdf:Property;
    rdfs:label "contributor";
    rdfs:comment "A person that has contributed to the CHO (but is not the only creator).";
    rdfs:subPropertyOf dcterms:contributor .

dm2e:editor a rdf:Property;
    rdfs:label "editor";
    rdfs:comment "The editor of a book,  journal or series. Can be a person or organization.";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:mentioned a rdf:Property;
    rdfs:label "mentioned";
    rdfs:comment "A person or instituion that is explicitly mentioned in the (textual) CHO." .

dm2e:subjectOf a rdf:Property;
    rdfs:label "subjectOf";
    rdfs:comment "Persons who are subjects of a CHO.";
    rdfs:range dm2e:Person .

dm2e:author a rdf:Property;
    rdfs:label "author";
    rdfs:comment "The author of the CHO.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dcterms:creator .

dm2e:painter a rdf:Property;
    rdfs:label "painter";
    rdfs:comment """A painter. In the manuscript context especially used in works of the 16th and 17th century
(rare books).""";
    rdfs:subPropertyOf dcterms:creator .

dm2e:locatedIn a rdf:Property;
    rdfs:label "locatedIn";
    rdfs:domain dm2e:Institution .

dm2e:copyist a rdf:Property;
    rdfs:label "copyist";
    rdfs:comment "Someone who copied a CHO.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dm2e:contributor .

dm2e:copyrightHolder a rdf:Property;
    rdfs:label "copyrightHolder";
    rdfs:comment "The person or institution that holds the copyright.";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:owner a rdf:Property;
    rdfs:label "owner";
    rdfs:comment "Indicates the ownership of a CHO.";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:wasTaughtBy a rdf:Property;
    rdfs:label "wasTaughtBy";
    rdfs:comment "The CHO was taught by an Agent." .

dm2e:hasVersion a rdf:Property;
    rdfs:label "hasVersion";
    rdfs:comment "Related resource that is an adaption of this resource";
    rdfs:range dm2e:Version .

dm2e:isPartOf a rdf:Property;
    rdfs:label "isPartOf";
    rdfs:comment "Related resource that incluedes this resource." .

dm2e:patron a rdf:Property;
    rdfs:label "patron";
    rdfs:comment "A person who is honored within the text.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dm2e:contributor .

dm2e:artist a rdf:Property;
    rdfs:label "artist";
    rdfs:comment "An artist that has created (e.g. painter) the CHO.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dcterms:creator .

dm2e:misattributed a rdf:Property;
    rdfs:label "misattributed";
    rdfs:comment "A person or institution that is mentioned as the creator of the CHO although he is not the creator." .

dm2e:honoree a rdf:Property;
    rdfs:label "honoree";
    rdfs:comment "An honored person, for whom the book is published.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dm2e:contributor .

dm2e:composer a rdf:Property;
    rdfs:label "composer";
    rdfs:comment "Person who has composed a CHO (e.g. a letter). Can be the same as the  person who wrote the letter, but can also be a different person, e.g. someone who has dictated but not wrote the letter.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dcterms:creator .

dm2e:writer a rdf:Property;
    rdfs:label "writer";
    rdfs:comment "Person who has written a CHO, e.g. a letter.";
    rdfs:range dm2e:Person;
    rdfs:subPropertyOf dcterms:creator .

dm2e:translator a rdf:Property;
    rdfs:label "translator";
    rdfs:comment "Someone who translated the CHO.";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:illustrator a rdf:Property;
    rdfs:label "illustrator";
    rdfs:comment "Someone who has made the illustrations of a CHO.";
    rdfs:subPropertyOf dcterms:creator .

dm2e:refersTo a rdf:Property;
    rdfs:label "refersTo";
    rdfs:comment """A person or CHO that is explicitly or implicitly menioned in the 
      (textual) CHO.""" .

dm2e:recipient a rdf:Property;
    rdfs:label "recipient";
    rdfs:comment "The recipient of a CHO, e.g. a letter.";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:influencedBy a rdf:Property;
    rdfs:label "influencedBy";
    rdfs:comment "Another agent by which the agent was influenced.";
    rdfs:range dm2e:Person .

dm2e:portrayed a rdf:Property;
    rdfs:label "portrayed";
    rdfs:comment "A person that is portrayed in the (textual or painted) CHO by its creator.";
    rdfs:range dm2e:Person .

dm2e:studentOf a rdf:Property;
    rdfs:label "studentOf";
    rdfs:comment "A teacher of the agent.";
    rdfs:range dm2e:Person .

dm2e:sponsor a rdf:Property;
    rdfs:label "sponsor";
    rdfs:comment "A person or an institution who has sponsored (parts) of the CHOs creation.";
    rdfs:subPropertyOf dm2e:contributor .

dm2e:wasStudiedBy a rdf:Property;
    rdfs:label "wasStudiedBy";
    rdfs:comment "The CHO was studied by an Agent." .

dm2e:publishedAt a rdf:Property, owl:FunctionalProperty;
    rdfs:label "publishedAt";
    rdfs:comment "The place of publication.";
    rdfs:subPropertyOf dcterms:spatial .
