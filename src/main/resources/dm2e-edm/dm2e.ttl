@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://dm2edata.hu-berlin.de/neologism/dm2e> a owl:Ontology;
    dcterms:title "DM2E Vocabulary";
    dcterms:description "The following specification document describes the DM2E data model in its second version. It extends the EDM+ v0.1 of the DM2E project and is the current extension of the EDM made by DM2E.";
    dcterms:modified "2013-01-31"^^xsd:date;
    vann:preferredNamespaceUri "http://dm2edata.hu-berlin.de/neologism/dm2e#";
    vann:preferredNamespacePrefix "dm2e";
    foaf:homepage <http://dm2edata.hu-berlin.de/neologism/dm2e.html>;
    dcterms:created "2012-12-04"^^xsd:date;
    dcterms:publisher "http://dm2edata.hu-berlin.de/neologism/dm2e#School%20for%20Library%20and%20Information%20Science";
    dcterms:isPartOf <http://dm2edata.hu-berlin.de/neologism>;
    dcterms:type <http://purl.org/adms/assettype/Ontology>;
    #dcterms:status <http://purl.org/adms/status/UnderDevelopment>;
    dc:creator
        dm2e:JIwanowa,
        dm2e:Evelyn,
        <http://dm2edata.hu-berlin.de/neologism/dm2e#s.hennicke> .

dm2e:ttl
    dcterms:FileFormat <> .

dm2e:rdf
    dcterms:FileFormat <> .

