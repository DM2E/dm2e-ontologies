    <!-- http://rdvocab.info/ElementsGr2/dateOfEstablishment -->

    <owl:ObjectProperty rdf:about="&rdaGr2;dateOfEstablishment">
        <skos:definition xml:lang="en">The date on which a corporate body was established or founded.</skos:definition>
        <scopeNote xml:lang="en">The date the institution was established.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;date"/>
        <rdfs:domain rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfTermination -->

    <owl:ObjectProperty rdf:about="&rdaGr2;dateOfTermination">
        <skos:definition xml:lang="en">The date on which a corporate body was terminated or dissolved.</skos:definition>
        <scopeNote xml:lang="en">The date the institution was terminated.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;date"/>
        <rdfs:domain rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/aggregatedCHO -->

    <owl:ObjectProperty rdf:about="&edm;aggregatedCHO">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="en">The property edm:aggregatedCHO connects the ore:Aggregation with exactly one edm:ProvidedCHO. The property expresses what the Aggregation is about.</scopeNote>
        <skos:definition xml:lang="en">This property associates an ORE aggregation with the cultural heritage object(s) (CHO for short) it is about.</skos:definition>
        <rdfs:range rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
        <rdfs:subPropertyOf rdf:resource="&ore;aggregates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/currentLocation -->

    <owl:ObjectProperty rdf:about="&edm;currentLocation">
        <scopeNote xml:lang="en">Current location of physical CHO, possibly a library building. If possible, use a resource here.</scopeNote>
        <skos:definition xml:lang="en">The geographic location and/or name of the repository, building, site, or other entity whose boundaries presently include the resource.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&dcterms;spatial"/>
        <rdfs:range rdf:resource="&edm;Place"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/dataProvider -->

    <owl:ObjectProperty rdf:about="&edm;dataProvider">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <skos:definition xml:lang="en">The name or identifier of the organisation who contributes data indirectly to an aggregation service (e.g. Europeana).</skos:definition>
        <scopeNote xml:lang="en">The property edm:dataProvider holds the name or identifier of the organisation (foaf:Organization, see below) that provided and owns the source metadata record for this aggregation. Note that this organisation does not necessarily own the physical object which is described in the metadata record.</scopeNote>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
        <rdfs:range rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/happenedAt -->

    <owl:ObjectProperty rdf:about="&edm;happenedAt">
        <scopeNote xml:lang="en">Connects an event to the place where this event happened at. </scopeNote>
        <skos:definition xml:lang="en">This property associates an event with the place at which the event happened.</skos:definition>
        <rdfs:domain rdf:resource="&edm;Event"/>
        <rdfs:range rdf:resource="&edm;Place"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/hasMet -->

    <owl:ObjectProperty rdf:about="&edm;hasMet">
        <scopeNote xml:lang="en">Unites properties with the range edm:Agent like dc:creator or dc:contributor.</scopeNote>
        <skos:definition xml:lang="en">edm:hasMet relates a resource with the objects or phenomena that have happened to or have happened together with the resource under consideration. We can abstractly think of history and the present as a series of “meetings” between people and other things in space-time. Therefore we name this relationship as the things the object “has met” in the course of its existence. These meetings are events in the proper sense, in which other people and things participate in any role.</skos:definition>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;Agent"/>
                    <rdf:Description rdf:about="&edm;Event"/>
                    <rdf:Description rdf:about="&edm;Place"/>
                    <rdf:Description rdf:about="&edm;TimeSpan"/>
                    <rdf:Description rdf:about="&skos;Concept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/hasView -->

    <owl:ObjectProperty rdf:about="&edm;hasView">
        <scopeNote xml:lang="en">The property edm:hasView holds the URL of a edm:WebResource which shows, 
    depicts or otherwise contains any kind of view of the ProvidedCHO.</scopeNote>
        <skos:definition xml:lang="en">This property relates a ORE aggregation about a CHO with a web resource providing a view of that CHO. Examples of view are: a thumbnail, a textual abstract and a table of contents. The ORE aggregation may be a Europeana aggregation, in which case the view is an object owned by Europeana (i.e., an instance of edm:EuropeanaObject) or an aggregation contributed by a content provider. In order to capture both these cases, the domain of edm:hasView is ore:Aggregation and its range is edm:WebResource.</skos:definition>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
        <rdfs:subPropertyOf rdf:resource="&ore;aggregates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isDerivativeOf -->

    <owl:ObjectProperty rdf:about="&edm;isDerivativeOf">
        <scopeNote xml:lang="en">Original version (in case of CHOs) or a related resource (in case of WebResources) from which this object has been derived in whole or in part.</scopeNote>
        <skos:definition xml:lang="en">This property captures a narrower notion of derivation than edm:isSimilarTo, in the sense that it relates a resource to another one, obtained by reworking, reducing, expanding, parts or the whole contents of the former, and possibly adding some minor parts. Versions have an even narrower meaning, in that it requires common identity between the related resources. Translations, summaries, abstractions etc. do not qualify as versions, but do qualify as derivatives.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&edm;isSimilarTo"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isNextInSenquence -->

    <owl:ObjectProperty rdf:about="&edm;isNextInSenquence">
        <scopeNote xml:lang="en">Can be used if a CHO has several parts of CHOs or WebResources in a sequence. </scopeNote>
        <skos:definition xml:lang="en">edm:isNextInSequence relates two resources S and R that are ordered parts of the same resource A, and such that S comes immediately after R in the order created by their being parts of A.</skos:definition>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isShownAt -->

    <owl:ObjectProperty rdf:about="&edm;isShownAt">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <skos:definition xml:lang="en">An unambiguous URL reference to the digital object on the provider’s website in its full information context. See also edm:isShownBy.</skos:definition>
        <scopeNote xml:lang="en">The property edm:isShownAt holds the URL of a edm:WebResource which leads to a view of the digital object on the provider&apos;s web site in its full information context (e.g. in a viewer application). Either edm:isShownBy or edm:isShownAt must be provided.</scopeNote>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isShownBy -->

    <owl:ObjectProperty rdf:about="&edm;isShownBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <skos:definition xml:lang="en">An unambiguous URL reference to the digital object on the provider’s web site in the best available resolution/quality. See also edm:isShownAt.</skos:definition>
        <scopeNote xml:lang="en">The property edm:isShownBy holds the URL of a edm:WebResource which leads to a &quot;plain&quot; image with any kind of view of the edm:ProvidedCHO. &quot;Plain&quot; image means that the image must be without any information context, for example, the URL points to a plain JPG-image. Either edm:isShownBy or edm:isShownAt must be provided.</scopeNote>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isSimilarTo -->

    <owl:ObjectProperty rdf:about="&edm;isSimilarTo">
        <skos:definition xml:lang="en">The most generic derivation property, covering also the case of questionable derivation. Is Similar To asserts that parts of the contents of one resource exhibit common features with respect to ideas, shapes, structures, colors, words, plots, topics with the contents of the related resource. Those common features may be attributed to a common origin or influence (in particular for derivation), but also to more generic cultural or psychological factors.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/object -->

    <owl:ObjectProperty rdf:about="&edm;object">
        <rdfs:comment>The URL of a suitable source image in the best resolution available on the web site of the data provider from which small images could be generated for use in a portal. This will often be the same URL as given in edm:isShownBy.</rdfs:comment>
        <scopeNote xml:lang="en">The property edm:object holds the URL of an edm:WebResource which leads to a thumbnail representing the digital object or, if there is no such thumbnail, the URL of the digital object in the best resolution available on the website of the data provider from which a thumbnail could be generated. This will often be the same URL as given in edm:isShownBy.
If you provide an edm:object it is mandatory to add one of the following MIME types with dc:format to the edm:WebResource:
•	image/png
•	image/jpeg
•	image/gif
•	image/tiff
•	application/pdf
</scopeNote>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/occurredAt -->

    <owl:ObjectProperty rdf:about="&edm;occurredAt">
        <scopeNote xml:lang="en">Connects an event to a time span which overlaps with the occurrence of that event. The occurrence in time of an event may overlap with 0 to many disjoint time spans, and a time span may have 0 to many events whose occurrences overlap with it.</scopeNote>
        <skos:definition xml:lang="en">This property associates an event to the smallest known time span that overlaps with the occurrence of that event.</skos:definition>
        <rdfs:domain rdf:resource="&edm;Event"/>
        <rdfs:range rdf:resource="&edm;TimeSpan"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/provider -->

    <owl:ObjectProperty rdf:about="&edm;provider">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <skos:definition xml:lang="en">The name or identifier of the organisation who delivers data directly to an aggregation service (e.g. Europeana).</skos:definition>
        <scopeNote xml:lang="en">The property edm:provider holds the name or identifier of the organisation  (foaf:Organization, see below) that provided the data, i.e. the aggregation, to Europeana. Note that this organisation is not necessarily owning the original or digitized object. Typically, edm:provider is an aggregator. The owner of the metadata record is recorded in edm:dataProvider. The values in edm:provider and edm:dataProvider can be the same. Note: In the context of DM2E the value for edm:provider is always &quot;DM2E&quot;.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
        <rdfs:range rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/wasPresentAt -->

    <owl:ObjectProperty rdf:about="&edm;wasPresentAt">
        <scopeNote xml:lang="en">An event where the agent was present at.</scopeNote>
        <skos:definition xml:lang="en">This property associates the people, things or information resources with an event at which they were present.</skos:definition>
        <rdfs:domain rdf:resource="&edm;Agent"/>
        <rdfs:range rdf:resource="&edm;Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openarchives.org/ore/terms/aggregates -->

    <owl:ObjectProperty rdf:about="&ore;aggregates">
        <rdfs:comment xml:lang="en">Aggregations, by definition, aggregate resources. The ore:aggregates relationship expresses that the object resource is a member of the set of Aggregated Resources of the subject (the Aggregation). This relationship between the Aggregation and its Aggregated Resources is thus more specific than a simple part/whole relationship, as expressed by dcterms:hasPart for example.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;hasPart"/>
        <rdfs:range rdf:resource="&ore;AggregatedResource"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.openarchives.org/ore/terms/describes -->

    <owl:ObjectProperty rdf:about="&ore;describes">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">This relationship asserts that the subject (a Resource Map) describes the object (an Aggregation).</rdfs:comment>
        <rdfs:range rdf:resource="&ore;Aggregation"/>
        <rdfs:domain rdf:resource="&ore;ResourceMap"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#inScheme -->

    <owl:ObjectProperty rdf:about="&skos;inScheme">
        <scopeNote xml:lang="en">The URI of a concept scheme.</scopeNote>
        <rdfs:domain rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://onto.dm2e.eu/schemas/dm2e/callNumber -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/callNumber">
        <scopeNote xml:lang="en">The call number for some archival item.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;identifier"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/condition -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/condition">
        <scopeNote xml:lang="en">Contains a description of the CHOs condition.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/cover -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/cover">
        <scopeNote xml:lang="en">Description of the cover of the CHO, e.g. the cover’s type of material.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/displayLevel -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/displayLevel">
        <rdfs:label xml:lang="en">displayLevel</rdfs:label>
        <scopeNote>The property with the value “true” should be added to the Level-CHO that should be displayed via the search engine. To all other Part-CHO the value is “false”.</scopeNote>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/explicit -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/explicit">
        <rdfs:label xml:lang="en">explicit</rdfs:label>
        <rdfs:comment xml:lang="en">Final words of a manuscript. A language tag in ISO 639-2 should be added to the Literal.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/illustration -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/illustration">
        <rdfs:label xml:lang="en">illustration</rdfs:label>
        <rdfs:comment xml:lang="en">Generic description of illustrations in the CHO.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/incipit -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/incipit">
        <rdfs:label xml:lang="en">incipit</rdfs:label>
        <rdfs:comment xml:lang="en">Opening words of a manuscript. A language tag in ISO 639-2 should be added to the Literal.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/levelOfGenesis -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/levelOfGenesis">
        <scopeNote xml:lang="en">The property holds additional information about the genesis of the CHO. Sometimes there are unfinished works or there are only drafts. This could be described more detailed here.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/levelOfHierarchy -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/levelOfHierarchy">
        <rdfs:label xml:lang="en">levelOfHierarchy</rdfs:label>
        <scopeNote xml:lang="en">The Top-Level-CHO must contain the number “1” as value of the dm2e:levelOfHierarchy property. All bottom hierarchy levels must be mark by incremental adding +1. For example Second-Level-CHO “2” and so on.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/modeOfAcquisition -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/modeOfAcquisition">
        <rdfs:label xml:lang="en">modeOfAcquisition</rdfs:label>
        <scopeNote xml:lang="en">holds meta data about the acquisition of the CHO (analogue to MAB field 664ba1).</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;provenance"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/origin -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/origin">
        <rdfs:label xml:lang="en">origin</rdfs:label>
        <scopeNote xml:lang="en">Provenance information about the origin of the physical CHO (analogue MAB field 664aa1).</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;provenance"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/pageDimension -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/pageDimension">
        <rdfs:label xml:lang="en">page dimension</rdfs:label>
        <scopeNote xml:lang="en">Page size. Please do also note the unit that was used.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;extent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/restoration -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/restoration">
        <scopeNote xml:lang="en">Contains information about the restoration status of the CHO.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/scopeNote -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/scopeNote">
        <rdfs:comment xml:lang="en">This property describes the dm2e-specific usage of the resource.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&skos;note"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/shelfmarkLocation -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/shelfmarkLocation">
        <rdfs:label xml:lang="en">shelfmark location</rdfs:label>
        <rdfs:comment xml:lang="en">Shelfmark location from the CHO.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/subtitle -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/subtitle">
        <scopeNote xml:lang="en">Any form of a subtitle.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;title"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/support -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/support">
        <scopeNote xml:lang="en">Description of the type of material of the physical CHO.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/watermark -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/watermark">
        <scopeNote xml:lang="en">Contains a description of a watermark which the CHO carries.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/writtenAreaDimension -->

    <owl:DatatypeProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/writtenAreaDimension">
        <rdfs:label xml:lang="en">written area dimension</rdfs:label>
        <scopeNote xml:lang="en">Size of writing or the part of the page where something is actually written. Please do also note the unit that was used.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;extent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:DatatypeProperty rdf:about="&dc;date">
        <skos:definition xml:lang="en">A point or period of time associated with an event in the lifecycle of the resource.</skos:definition>
        <scopeNote xml:lang="en">A significant date associated with this resource. The date should be described with edm:Event.</scopeNote>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:DatatypeProperty rdf:about="&dc;description">
        <skos:definition xml:lang="en">An account of the resource.</skos:definition>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/format -->

    <owl:DatatypeProperty rdf:about="&dc;format">
        <skos:definition xml:lang="en">The file format, physical medium, or dimensions of the resource.</skos:definition>
        <scopeNote>With domain edm:WebResource: Format of the Web resource, e.g. PDF, HTML or JPG. If annotatable content is described, please use one of the following specifications: “text/html-named-content” for HTML content as specified by Pundit, “text/plain” for plain text content, “text/html” for websites with text and images, or “image/gif”, “image/jpeg” resp. “image/png” for Image files. If edm:object is described, please use one these MIME types: “image/png”, “image/jpeg”, “image/gif”, “image/tiff” for image files or “application/pdf” for pdf. See the technical specification “DM2E Annotatable Content” or the “Recommendations for the RDF representation of DM2E metadata” for further information.
With domain edm:ProvidedCHO see skos:definition.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:domain rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasType"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/identifier -->

    <owl:DatatypeProperty rdf:about="&dc;identifier">
        <skos:definition xml:lang="en">An unambiguous reference to the resource within a given context.</skos:definition>
        <scopeNote xml:lang="en">Identifier of the resource.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;Agent"/>
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/language -->

    <owl:DatatypeProperty rdf:about="&dc;language">
        <skos:definition xml:lang="en">A language of the resource.</skos:definition>
        <scopeNote xml:lang="en">The property dc:language holds the most prominent language of the CHO. If an edm:type property with the value “TEXT” has been given, it is mandatory to provide at least one language of the CHO. If several languages are present in the CHO then repeat dc:language for each language. Regarding the RFC 3306 recommendations, the ISO 639-1 2-character code should be used for this property. When a language has no 2-character code, and there are two different ISO-2 3-character codes, the ISO 639-2/T (Terminology) code should be added.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&edm;hasType"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/relation -->

    <owl:DatatypeProperty rdf:about="&dc;relation">
        <rdfs:subPropertyOf rdf:resource="&edm;isRelatedTo"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:DatatypeProperty rdf:about="&dc;rights">
        <skos:definition xml:lang="en">Information about rights held in and over the resource.</skos:definition>
        <scopeNote xml:lang="en">URL of a resource describing licensing rights of the CHO.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:DatatypeProperty rdf:about="&dc;title">
        <skos:definition xml:lang="en">A name given to the resource.</skos:definition>
        <scopeNote xml:lang="en">The property dc:titles gives the main title of the CHO. It is mandatory to provide either a dc:title or a dc:description for the CHO. A language tag in ISO 639-2 should be added to the Literal.</scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/alternative -->

    <owl:DatatypeProperty rdf:about="&dcterms;alternative">
        <skos:definition xml:lang="en">An alternative name for the resource.</skos:definition>
        <scopeNote xml:lang="en">An alternative name for the resource. Should not be used for subtitles in DM2E. Can be used if title and subtitle are in one field. A language tag in ISO 639-2 should be added to the Literal.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;title"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:DatatypeProperty rdf:about="&dcterms;created">
        <skos:definition xml:lang="en">Date of creation of the resource.</skos:definition>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/extent -->

    <owl:DatatypeProperty rdf:about="&dcterms;extent">
        <scopeNote xml:lang="en">The size or duration of the resource.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;format"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:DatatypeProperty rdf:about="&dcterms;issued">
        <skos:definition xml:lang="en">Date of formal issuance (e.g., publication) of the resource.</skos:definition>
        <scopeNote xml:lang="en">Date of publication (of the described CHO, usually the original one).</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/medium -->

    <owl:DatatypeProperty rdf:about="&dcterms;medium">
        <skos:definition xml:lang="en">The material or physical carrier of the CHO. &lt;dcterms:medium&gt;metal&lt;/dcterms:medium&gt;</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&dc;format"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/modified -->

    <owl:DatatypeProperty rdf:about="&dcterms;modified">
        <skos:definition xml:lang="en">Date on which the resource was changed.</skos:definition>
        <scopeNote xml:lang="en">The property dcterms:created holds the modification date and time of the aggregation, i.e. the original source metadata record.</scopeNote>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/provenance -->

    <owl:DatatypeProperty rdf:about="&dcterms;provenance">
        <skos:definition xml:lang="en">A statement of any changes in ownership and custody of the resource since its creation that are significant for its authenticity, integrity, and interpretation.</skos:definition>
        <scopeNote xml:lang="en">Is used for mixed provenance data that cannot be mapped to the properties dm2e:origin or dm2e:modeOfAcquisition.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/tableOfContents -->

    <owl:DatatypeProperty rdf:about="&dcterms;tableOfContents">
        <skos:definition xml:lang="en">A list of subunits of the resource.</skos:definition>
        <scopeNote xml:lang="en">Any kind of table of contents for the CHO.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/isbn -->

    <owl:DatatypeProperty rdf:about="&bibo;isbn">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="en">The ISBN number for the CHO of type book.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;identifier"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/issn -->

    <owl:DatatypeProperty rdf:about="&bibo;issn">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="en">The ISSN number for the CHO of type journal.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;identifier"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/numPages -->

    <owl:DatatypeProperty rdf:about="&bibo;numPages">
        <rdfs:comment>The number of pages contained in a document.</rdfs:comment>
        <scopeNote xml:lang="en">A string of non-contiguous page spans that locate a Document within a Collection. Example: 23-25, 34, 54-56.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;extent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:range rdf:resource="&xsd;unsignedInt"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/numVolumes -->

    <owl:DatatypeProperty rdf:about="&bibo;numVolumes">
        <rdfs:comment>The number of volumes contained in a collection of documents (usually a series, periodical, etc.).</rdfs:comment>
        <scopeNote xml:lang="en">The number of volumes contained in a collection of documents (usually a series, periodical, etc.).</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;extent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:range rdf:resource="&xsd;unsignedInt"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/number -->

    <owl:DatatypeProperty rdf:about="&bibo;number">
        <rdfs:comment xml:lang="en">A generic item or document number. Not to be confused with issue number.</rdfs:comment>
        <scopeNote xml:lang="en">The property bibo:number is used to store the page number of each CHO from type dm2e:Page. On the higher Level-CHOs can be added the normalized form of the number.  </scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/pages -->

    <owl:DatatypeProperty rdf:about="&bibo;pages">
        <rdfs:comment xml:lang="en">A string of non-contiguous page spans that locate a Document within a Collection. Example: 23-25, 34, 54-56. […]</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;description"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/suffixName -->

    <owl:DatatypeProperty rdf:about="&bibo;suffixName">
        <scopeNote xml:lang="en">Persons names or notations about organisations including counting, for example Johannes Paul, I.</scopeNote>
        <rdfs:comment xml:lang="en">The suffix of the name.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&skos;note"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/volume -->

    <owl:DatatypeProperty rdf:about="&bibo;volume">
        <scopeNote xml:lang="en">A volume number.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;identifier"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:range rdf:resource="&xsd;unsignedInt"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/biographicalInformation -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;biographicalInformation">
        <skos:definition xml:lang="en">Information pertaining to the life or history of a person.</skos:definition>
        <scopeNote xml:lang="en">Information related to the biography of the agent.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfBirth -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;dateOfBirth">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="en">The date the person was born.</scopeNote>
        <skos:definition xml:lang="en">The year a person was born. Date of birth may also include the month and day of the person’s birth.</skos:definition>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfDeath -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;dateOfDeath">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="en">The date the person died.</scopeNote>
        <skos:definition xml:lang="en">The year a person died. Date of death may also include the month and day of the person’s death.</skos:definition>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfEstablishment -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;dateOfEstablishment">
        <skos:definition xml:lang="en">The date on which a corporate body was established or founded.</skos:definition>
        <scopeNote xml:lang="en">The date the institution was established.</scopeNote>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfTermination -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;dateOfTermination">
        <skos:definition xml:lang="en">The date on which a corporate body was terminated or dissolved.</skos:definition>
        <scopeNote xml:lang="en">The date the institution was terminated.</scopeNote>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/gender -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;gender">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="en">The gender of the person.</scopeNote>
        <skos:definition xml:lang="en">The gender with which a person identifies.</skos:definition>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/otherDesignationAssociatedWithTheCorporateBody -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;otherDesignationAssociatedWithTheCorporateBody">
        <skos:definition xml:lang="en">A term other than a title that is associated with a person’s name.</skos:definition>
        <scopeNote xml:lang="en">Additional names. </scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/otherDesignationAssociatedWithThePerson -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;otherDesignationAssociatedWithThePerson">
        <skos:definition xml:lang="en">A term other than a title that is associated with a person’s name.</skos:definition>
        <scopeNote xml:lang="en">Additional names.</scopeNote>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/professionOrOccupation -->

    <owl:DatatypeProperty rdf:about="&rdaGr2;professionOrOccupation">
        <skos:definition xml:lang="en">A profession or occupation in which a person works or has worked.</skos:definition>
        <scopeNote xml:lang="en">The profession or occupation in which the person works or has worked.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/P79F.beginning_is_qualified_by -->

    <owl:DatatypeProperty rdf:about="&crm;P79F.beginning_is_qualified_by">
        <rdfs:comment xml:lang="en">Qualifying information about the end of the timespan – such as degree of certainty, precision, source etc.</rdfs:comment>
        <scopeNote xml:lang="en">This property is used to indicate the certainty of a timespan, in this case its end. Use “uncertainty_data” or “uncertainty_granularity” to indicate the type of uncertainty. More information can be found in the “Recommendations for the RDF representation of DM2E metadata”.</scopeNote>
        <rdfs:domain rdf:resource="&edm;TimeSpan"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/P80F.end_is_qualified_by -->

    <owl:DatatypeProperty rdf:about="&crm;P80F.end_is_qualified_by">
        <rdfs:comment xml:lang="en">Qualifying information about the start of the timespan – such as degree of certainty, precision, source etc.
</rdfs:comment>
        <scopeNote xml:lang="en">This property is used to indicate the certainty of a timespan, in this case its beginning. Use “uncertainty_data” or “uncertainty_granularity” to indicate the type of uncertainty. More information can be found in the “Recommendations for the RDF representation of DM2E metadata”</scopeNote>
        <rdfs:domain rdf:resource="&edm;TimeSpan"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/begin -->

    <owl:DatatypeProperty rdf:about="&edm;begin">
        <scopeNote xml:lang="en">The beginning of a timespan.</scopeNote>
        <skos:definition xml:lang="en">This property denotes the start date of a period of time.</skos:definition>
        <rdfs:domain rdf:resource="&edm;TimeSpan"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/end -->

    <owl:DatatypeProperty rdf:about="&edm;end">
        <scopeNote xml:lang="en">The end of a timespan.</scopeNote>
        <skos:definition xml:lang="en">This property denotes the end date of a period of time.</skos:definition>
        <rdfs:domain rdf:resource="&edm;TimeSpan"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/hasType -->

    <owl:DatatypeProperty rdf:about="&edm;hasType">
        <skos:definition xml:lang="en">This property relates a resource with the concepts it belongs to in a suitable type system such as MIME or any thesaurus that captures categories of objects in a given field (e.g., the âObjectsâ facet in Gettyâs Art and Architecture Thesaurus). It does not capture aboutness.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&edm;isRelatedTo"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isRelatedTo -->

    <owl:DatatypeProperty rdf:about="&edm;isRelatedTo">
        <scopeNote xml:lang="en">Any other related resource.</scopeNote>
        <skos:definition xml:lang="en">edm:isRelatedTo is the most general contextual property in EDM. Contextual properties have typically to do either with the things that have happened to or together with the object under consideration, or what the object refers to by its shape, form or features in a figural or encoded form. For sake of simplicity, we include in the contextual relationships also the scholarly classification, which may have either to do with the role and cultural connections of the object in the past, or its kind of structure, substance or contents as it can be verified at present.</skos:definition>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/rights -->

    <owl:DatatypeProperty rdf:about="&edm;rights">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <skos:definition xml:lang="en">Information about usage and access rights of the digital objects indicated in isShownBy and isShownAt.</skos:definition>
        <scopeNote xml:lang="en">The property edm:rights holds copyright information pertaining to all digital objects (edm:WebResource) given by edm:hasView or one of its subproperties. If a digital object (edm:WebResource) holds an additional copyright information given by dc:rights then this more specific copyright information has priority. Otherwise the value given in edm:rights at the Aggregation is the default value for digital object attached to this Aggregation. The four main types of rights statements in Europeana are: Public Domain Mark, CCO 1.0, Rights Reserved Statements and Unknown. The list of the all available licensing rights supported by Europeana can be found here: http://pro.europeana.eu/available-rights-statements.</scopeNote>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;WebResource"/>
                    <rdf:Description rdf:about="&ore;Aggregation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/type -->

    <owl:DatatypeProperty rdf:about="&edm;type">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <skos:definition xml:lang="en">The Europeana material type of the resource.</skos:definition>
        <scopeNote xml:lang="en">The Europeana material type of the resource. Must be one of the following Europeana types (in upper case): TEXT, VIDEO, SOUND, IMAGE, 3D.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first rdf:datatype="&xsd;string">3D</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first rdf:datatype="&xsd;string">IMAGE</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first rdf:datatype="&xsd;string">SOUND</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first rdf:datatype="&xsd;string">TEXT</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first rdf:datatype="&xsd;string">VIDEO</rdf:first>
                                                        <rdf:rest rdf:resource="&rdf;nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#alt -->

    <owl:DatatypeProperty rdf:about="&wgs84_pos;alt">
        <skos:definition xml:lang="en">The WGS84 altitude of a SpatialThing (decimal meters above the local reference ellipsoid).</skos:definition>
        <scopeNote xml:lang="en">The altitude of a place (decimal metres above the reference).</scopeNote>
        <rdfs:domain rdf:resource="&edm;Place"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#lat -->

    <owl:DatatypeProperty rdf:about="&wgs84_pos;lat">
        <skos:definition xml:lang="en">The WGS84 latitude of a SpatialThing (decimal degrees).”
Definition: “The WGS84 longitude of a SpatialThing (decimal degrees).
</skos:definition>
        <scopeNote xml:lang="en">The latitude of a place (decimal degree).</scopeNote>
        <rdfs:domain rdf:resource="&edm;Place"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#long -->

    <owl:DatatypeProperty rdf:about="&wgs84_pos;long">
        <skos:definition xml:lang="en">The WGS84 longitude of a SpatialThing (decimal degrees).</skos:definition>
        <scopeNote xml:lang="en">The longitude of a place (decimal degree).</scopeNote>
        <rdfs:domain rdf:resource="&edm;Place"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:DatatypeProperty rdf:about="&skos;altLabel">
        <skos:definition xml:lang="en">An alternative lexical label for a resource.</skos:definition>
        <scopeNote xml:lang="en">An alternative name, e.g. a former name or the name in another form.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#broader -->

    <owl:DatatypeProperty rdf:about="&skos;broader">
        <skos:definition xml:lang="en">Relates a concept to a concept that is more general in meaning.</skos:definition>
        <scopeNote xml:lang="en">URI of the broader concept.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&skos;Concept"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#hiddenLabel -->

    <owl:DatatypeProperty rdf:about="&skos;hiddenLabel">
        <skos:definition xml:lang="en">A lexical label for a resource that should be hidden when generating visual displays of the resource, but should still be accessible to free text search operations.</skos:definition>
        <scopeNote xml:lang="en">Non-standard forms of the name, e.g. misspellings.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#narrower -->

    <owl:DatatypeProperty rdf:about="&skos;narrower">
        <skos:definition xml:lang="en">Relates a concept to a concept that is more specific in meaning.</skos:definition>
        <scopeNote xml:lang="en">URI of the narrower concept.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&skos;Concept"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#notation -->

    <owl:DatatypeProperty rdf:about="&skos;notation">
        <skos:definition xml:lang="en">A notation, also known as classification code, is a string of characters such as “T58.5” or “303.4833” used to uniquely identify a concept within the scope of a given concept scheme.</skos:definition>
        <scopeNote xml:lang="en">If available: A notation of the concept.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&skos;Concept"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <owl:DatatypeProperty rdf:about="&skos;note">
        <skos:definition xml:lang="en">A general note, for any purpose.</skos:definition>
        <scopeNote xml:lang="en">Information related to the resource that cannot be modelled with other properties of the class.</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:DatatypeProperty rdf:about="&skos;prefLabel">
        <skos:definition xml:lang="en">The preferred lexical label for a resource, in a given language.</skos:definition>
        <scopeNote xml:lang="en">The preferred name of the agent, preferably in a normalized form. Only one preferred label per language tag is allowed!</scopeNote>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Archive -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Archive">
        <rdfs:label xml:lang="en">Archive</rdfs:label>
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
        <rdfs:comment xml:lang="en">Archives are places where public records and other historical documents are preserved.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Collection -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Collection">
        <rdfs:subClassOf rdf:resource="&edm;NonInformationResource"/>
        <rdfs:comment xml:lang="en">The collection of CHOs as provided by a data provider.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/DataResource -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/DataResource">
        <rdfs:subClassOf rdf:resource="&foaf;Document"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&void;inDataset"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The dataset that contains the RDF statements provided in via this data resource.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Document -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Document">
        <rdfs:label xml:lang="en">Document</rdfs:label>
        <rdfs:subClassOf rdf:resource="&edm;PhysicalThing"/>
        <rdfs:comment xml:lang="en">ProvidedCHO of type document. Unlike foaf:Document, dm2e:Document refers to a physical document.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/File -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/File">
        <rdfs:label xml:lang="en">File</rdfs:label>
        <rdfs:subClassOf rdf:resource="&edm;PhysicalThing"/>
        <rdfs:comment xml:lang="en">Archival Item.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Manuscript -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Manuscript">
        <rdfs:label xml:lang="en">Manuscript</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/Document"/>
        <rdfs:comment xml:lang="en">ProvidedCHO of type manuscript, e.g. Wittgensteins brown book. Not equivalent to bibo:Manuscript. Not equivalent to bibo:Manuscript.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Page -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Page">
        <rdfs:label xml:lang="en">Page</rdfs:label>
        <rdfs:subClassOf rdf:resource="&edm;PhysicalThing"/>
        <rdfs:comment xml:lang="en">A sheet of paper. Can be part of another CHO, e.g. dm2e:Manuscript.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Paragraph -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Paragraph">
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en">ProvidedCHO of type paragraph. Can be part of another (textual) CHO, e.g. dm2e:Manuscript.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Photo -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Photo">
        <rdfs:label xml:lang="en">Photo</rdfs:label>
        <rdfs:subClassOf rdf:resource="&foaf;Image"/>
        <rdfs:comment xml:lang="en">ProvidedCHO of type photo.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Publication -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Publication">
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en">ProvidedCHO of type publication.</rdfs:comment>
    </owl:Class>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/Work -->

    <owl:Class rdf:about="http://onto.dm2e.eu/schemas/dm2e/Work">
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en">A non-physical piece of work of an agent, e.g. the theoretical concept of a manuscript.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Book -->

    <owl:Class rdf:about="&bibo;Book">
        <rdfs:subClassOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/Document"/>
        <rdfs:comment xml:lang="en">A written or printed work of fiction or nonfiction, usually on sheets of paper fastened or bound together within covers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Issue -->

    <owl:Class rdf:about="&bibo;Issue">
        <rdfs:subClassOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/Document"/>
        <scopeNote>An issue of a journal.</scopeNote>
        <rdfs:comment>Something that is printed or published and distributed, esp. a given number of a periodical.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Journal -->

    <owl:Class rdf:about="&bibo;Journal">
        <rdfs:subClassOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/Document"/>
        <rdfs:comment xml:lang="en">A periodical of scholarly journal Articles.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Letter -->

    <owl:Class rdf:about="&bibo;Letter">
        <rdfs:subClassOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/Document"/>
        <rdfs:comment xml:lang="en">A written or printed communication addressed to a person or organization and usually transmitted by mail.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ontology/bibo/Series -->

    <owl:Class rdf:about="&bibo;Series">
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <scopeNote xml:lang="en">A collection of documents, based on some shared aspects, like a topic or provenance etc.</scopeNote>
        <rdfs:comment xml:lang="en">A loose, thematic, collection of Documents, often Books.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Article -->

    <owl:Class rdf:about="&fabio;Article">
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en">The realization of a piece of writing on a particular topic.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Chapter -->

    <owl:Class rdf:about="&fabio;Chapter">
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en">A defined document section, forming part of or intended for inclusion within a larger document, usually with its own title or chapter number. Different chapters within a document such as a book or a report may each be independently authored, or may all be authored by a single individual or group of authors.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Cover -->

    <owl:Class rdf:about="&fabio;Cover">
        <rdfs:subClassOf rdf:resource="&edm;PhysicalThing"/>
        <rdfs:comment xml:lang="en">A protective covering used to bind together the pages of a document or the first, informative, page of a digital document.</rdfs:comment>
        <scopeNote xml:lang="en">ProvidedCHO of type cover. Can be part of another CHO, e.g. a book.</scopeNote>
    </owl:Class>
    


    <!-- http://rdfs.org/ns/void#Dataset -->

    <owl:Class rdf:about="&void;Dataset">
        <scopeNote xml:lang="en">A void:Dataset represents a stable version of an RDF graph that contains descriptions of ore:Aggregations and edm:providedCHOs of one data provider.</scopeNote>
    </owl:Class>
    


    <!-- http://vivoweb.org/ontology/core#Library -->

    <owl:Class rdf:about="&vivo;Library">
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
        <rdfs:comment xml:lang="en">A collection of any materials, for study and enjoyment, which has been systematically arranged.</rdfs:comment>
    </owl:Class>
    


    <!-- http://vivoweb.org/ontology/core#Museum -->

    <owl:Class rdf:about="&vivo;Museum">
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
        <rdfs:comment xml:lang="en">A building or place where works of art, scientific specimens, or other objects of permanent value are kept and displayed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://vivoweb.org/ontology/core#University -->

    <owl:Class rdf:about="&vivo;University">
        <rdfs:subClassOf rdf:resource="&foaf;Organization"/>
        <rdfs:comment xml:lang="en">An institution of higher education and research, which grants academic degrees in a variety of subjects, and provides both undergraduate education and postgraduate education.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/Agent -->

    <owl:Class rdf:about="&edm;Agent">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&skos;prefLabel"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&edm;NonInformationResource"/>
        <owl:disjointWith rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en">This class comprises people, either individually or in groups, who have the potential to perform intentional actions for which they can be held responsible.</skos:definition>
        <scopeNote xml:lang="en">cf. original scope note</scopeNote>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/Event -->

    <owl:Class rdf:about="&edm;Event">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&skos;prefLabel"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&edm;NonInformationResource"/>
        <scopeNote xml:lang="en">“An event is a change ‘of states in cultural, social or physical systems, regardless of scale, brought about by a series or group of coherent physical, cultural, technological or legal phenomena’ (E5 Event in CIDOC CRM) or a ‘set of coherent phenomena or cultural manifestations bounded in time and space’ (E4 Period in CIDOC CRM).” (Definition of the Europeana Data Model elements, 2013:11).</scopeNote>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/InformationResource -->

    <owl:Class rdf:about="&edm;InformationResource">
        <skos:definition xml:lang="en">An information resource is a resource whose essential characteristics can be conveyed in a single message. It can be associated with a URI, it can have a representation, for example: a text is an InformationResource.</skos:definition>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/NonInformationResource -->

    <owl:Class rdf:about="&edm;NonInformationResource">
        <owl:equivalentClass>
            <owl:Class>
                <owl:complementOf rdf:resource="&edm;InformationResource"/>
            </owl:Class>
        </owl:equivalentClass>
        <skos:definition xml:lang="en">All resources that are not information resources.</skos:definition>
        <scopeNote xml:lang="en">“This class comprises people, either individually or in groups, who have the potential to perform intentional actions for which they can be held responsible.” (Definition of the Europeana Data Model elements, 2013:9).</scopeNote>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/PhysicalThing -->

    <owl:Class rdf:about="&edm;PhysicalThing">
        <rdfs:subClassOf rdf:resource="&edm;NonInformationResource"/>
        <skos:definition xml:lang="en">A persistent physical item such as a painting, a building, a book or a stone. Persons are not items. This class represents Cultural Heritage Objects known to Europeana to be physical things (such as Mona Lisa) as well as all physical things Europeana refers to in the descriptions of Cultural Heritage Objects (such as the Rosetta Stone).</skos:definition>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/Place -->

    <owl:Class rdf:about="&edm;Place">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                        <owl:allValuesFrom rdf:resource="&edm;Place"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;isPartOf"/>
                        <owl:allValuesFrom rdf:resource="&edm;Place"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&skos;prefLabel"/>
                        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&wgs84_pos;alt"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&wgs84_pos;lat"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&wgs84_pos;long"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&edm;NonInformationResource"/>
        <skos:definition xml:lang="en">An “extent in space, in particular on the surface of the earth, in the pure sense of physics: independent from temporal phenomena and matter” (CIDOC CRM).</skos:definition>
        <rdfs:comment xml:lang="en">Places are identified by the content provider and named according to some vocabulary or local convention, and possibly normalized by Europeana at enrichment or at ingestion time. This class is the range of property edm:happenedAt.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/ProvidedCHO -->

    <owl:Class rdf:about="&edm;ProvidedCHO">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&dc;description"/>
                                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&dc;title"/>
                                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                        <owl:allValuesFrom rdf:resource="&edm;ProvidedCHO"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;isPartOf"/>
                        <owl:allValuesFrom rdf:resource="&edm;ProvidedCHO"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;isDerivativeOf"/>
                        <owl:allValuesFrom rdf:resource="&edm;ProvidedCHO"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dc;type"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/printedAt"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/publishedAt"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/shelfmarkLocation"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;spatial"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;currentLocation"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dc;language"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/callNumber"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/condition"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/levelOfGenesis"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://onto.dm2e.eu/schemas/dm2e/watermark"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bibo;isbn"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bibo;issn"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bibo;numPages"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bibo;numVolumes"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bibo;number"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&bibo;pages"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <scopeNote xml:lang="en">The EDM Definitions 5.2.4 describes the edm:ProvidedCHO as follows: “This class comprises the Cultural Heritage objects that Europeana collects descriptions about.” (Definition of the Europeana Data Model elements, 2013:13).
</scopeNote>
        <skos:definition xml:lang="en">This class comprises the Cultural Heritage objects that Europeana collects descriptions about.</skos:definition>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/TimeSpan -->

    <owl:Class rdf:about="&edm;TimeSpan">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                        <owl:allValuesFrom rdf:resource="&edm;TimeSpan"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;isPartOf"/>
                        <owl:allValuesFrom rdf:resource="&edm;TimeSpan"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&skos;prefLabel"/>
                        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;begin"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;end"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&edm;NonInformationResource"/>
        <skos:definition xml:lang="en">The class of “abstract temporal extents, in the sense of Galilean physics, having a beginning, an end and a duration” (CIDOC CRM).</skos:definition>
        <rdfs:comment xml:lang="en">This class is the range of edm:occurredAt. Time spans are identified by the content provider or by Europeana at enrichment time.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.europeana.eu/schemas/edm/WebResource -->

    <owl:Class rdf:about="&edm;WebResource">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                        <owl:allValuesFrom rdf:resource="&edm;WebResource"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;isPartOf"/>
                        <owl:allValuesFrom rdf:resource="&edm;WebResource"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;isDerivativeOf"/>
                        <owl:allValuesFrom rdf:resource="&edm;WebResource"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;isNextInSenquence"/>
                        <owl:allValuesFrom rdf:resource="&edm;WebResource"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&edm;rights"/>
                        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&edm;InformationResource"/>
