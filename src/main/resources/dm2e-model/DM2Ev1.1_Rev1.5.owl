    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://onto.dm2e.eu/schemas/dm2e/scopeNote -->

    <owl:AnnotationProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/scopeNote">
        <rdfs:comment xml:lang="en">This property describes the dm2e-specific usage of the resource.</rdfs:comment>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <rdf:Description rdf:about="&dc;creator">
        <skos:definition xml:lang="en">An entity primarily responsible for making the resource.</skos:definition>
        <scopeNote xml:lang="en">The property dcterms:creator holds the name or identifier of the agent (a person or organisation) who created the aggregation, i.e. the original metadata record, or CHO (possibly its author).</scopeNote>
    </rdf:Description>
    


    <!-- http://purl.org/dc/elements/1.1/modified -->

    <owl:AnnotationProperty rdf:about="&dc;modified"/>
    


    <!-- http://purl.org/dc/elements/1.1/subject -->

    <owl:AnnotationProperty rdf:about="&dc;subject">
        <scopeNote xml:lang="en">Subject of the CHO. Can be taken from another vocabulary. May also relate to a person, concept, place or timespan that is the subject of a CHO. The mapped metadata describes content-based information.</scopeNote>
        <skos:definition xml:lang="en">The topic of the resource.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&edm;isRelatedTo"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title">
        <skos:definition xml:lang="en">A name given to the resource.</skos:definition>
        <scopeNote xml:lang="en">The property dc:titles gives the main title of the CHO. It is mandatory to provide either a dc:title or a dc:description for the CHO. A language tag in ISO 639-2 should be added to the Literal.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/alternative -->

    <owl:AnnotationProperty rdf:about="&dcterms;alternative">
        <skos:definition xml:lang="en">An alternative name for the resource.</skos:definition>
        <scopeNote xml:lang="en">An alternative name for the resource. Should not be used for subtitles in DM2E. Can be used if title and subtitle are in one field. A language tag in ISO 639-2 should be added to the Literal.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:AnnotationProperty rdf:about="&dcterms;issued">
        <skos:definition xml:lang="en">Date of formal issuance (e.g., publication) of the resource.</skos:definition>
        <scopeNote xml:lang="en">Date of publication (of the described CHO, usually the original one).</scopeNote>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/ontology/bibo/suffixName -->

    <owl:AnnotationProperty rdf:about="&bibo;suffixName">
        <scopeNote xml:lang="en">Persons names or notations about organisations including counting, for example Johannes Paul, I.</scopeNote>
        <rdfs:comment xml:lang="en">The suffix of the name.</rdfs:comment>
        <rdfs:domain rdf:resource="&edm;Agent"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/vocab/vann/changes -->

    <owl:AnnotationProperty rdf:about="&vann;changes"/>
    


    <!-- http://purl.org/vocab/vann/example -->

    <owl:AnnotationProperty rdf:about="&vann;example"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="&vann;preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="&vann;preferredNamespaceUri"/>
    


    <!-- http://www.europeana.eu/schemas/edm/isRelatedTo -->

    <owl:AnnotationProperty rdf:about="&edm;isRelatedTo">
        <scopeNote xml:lang="en">Any other related resource.</scopeNote>
        <skos:definition xml:lang="en">edm:isRelatedTo is the most general contextual property in EDM. Contextual properties have typically to do either with the things that have happened to or together with the object under consideration, or what the object refers to by its shape, form or features in a figural or encoded form. For sake of simplicity, we include in the contextual relationships also the scholarly classification, which may have either to do with the role and cultural connections of the object in the past, or its kind of structure, substance or contents as it can be verified at present.</skos:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="&skos;altLabel">
        <skos:definition xml:lang="en">An alternative lexical label for a resource.</skos:definition>
        <scopeNote xml:lang="en">An alternative name, e.g. a former name or the name in another form.</scopeNote>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="&skos;definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#hiddenLabel -->

    <owl:AnnotationProperty rdf:about="&skos;hiddenLabel">
        <skos:definition xml:lang="en">A lexical label for a resource that should be hidden when generating visual displays of the resource, but should still be accessible to free text search operations.</skos:definition>
        <scopeNote xml:lang="en">Non-standard forms of the name, e.g. misspellings.</scopeNote>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <owl:AnnotationProperty rdf:about="&skos;note">
        <skos:definition xml:lang="en">A general note, for any purpose.</skos:definition>
        <scopeNote xml:lang="en">Information related to the resource that cannot be modelled with other properties of the class.</scopeNote>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="&skos;prefLabel">
        <skos:definition xml:lang="en">The preferred lexical label for a resource, in a given language.</skos:definition>
        <scopeNote xml:lang="en">The preferred name of the agent, preferably in a normalized form. Only one preferred label per language tag is allowed!</scopeNote>
        <rdfs:domain rdf:resource="http://onto.dm2e.eu/schemas/dm2e/#genid85"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/ns/adms#relatedWebPage -->

    <owl:AnnotationProperty rdf:about="&adms;relatedWebPage"/>
    


    <!-- http://www.w3.org/ns/radion#versionNotes -->

    <owl:AnnotationProperty rdf:about="&radion;versionNotes"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:AnnotationProperty rdf:about="&foaf;homepage"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://onto.dm2e.eu/schemas/dm2e/artist -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/artist">
        <rdfs:label xml:lang="en">artist</rdfs:label>
        <scopeNote xml:lang="en">An artist that has created (e.g. painted) the CHO.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;creator"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/composer -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/composer">
        <rdfs:label xml:lang="en">composer</rdfs:label>
        <scopeNote xml:lang="en">The person that has composed a CHO (e.g. a letter). Can be the same as the person who wrote the letter, but can also be a different person, e.g. someone who has dictated but not wrote the letter.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;creator"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/copyist -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/copyist">
        <rdfs:label xml:lang="en">copyist</rdfs:label>
        <scopeNote xml:lang="en">Someone who copied a CHO.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/genre -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/genre">
        <rdfs:label xml:lang="en">genre</rdfs:label>
        <scopeNote xml:lang="en">Genre of the CHO, in German “Formschlagworte”. Examples: schoolbook, letter collection.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;type"/>
        <rdfs:domain rdf:resource="&skos;Concept"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/hasAnnotatableVersionAt -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/hasAnnotatableVersionAt">
        <scopeNote xml:lang="en">The property dm2e:hasAnnotatableVersionAt holds an URL which leads to an HTML representation or to an image of the content, i.e. of the CHO aggregated by the current Aggregation. The URL of the HTML content or image file must be stable. The type of the WebResource must be further specified as “text/html-named-content”, “text/plain”, &quot;text/html&quot;, “image/gif”, “image/jpeg” or “image/png”.</scopeNote>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/honoree -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/honoree">
        <rdfs:label xml:lang="en">honoree</rdfs:label>
        <scopeNote xml:lang="en">An honoured person, for whom the CHO is published or created.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/influencedBy -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/influencedBy">
        <rdfs:label xml:lang="en">influenced by</rdfs:label>
        <scopeNote xml:lang="en">Another person by which the person was influenced.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/mentioned -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/mentioned">
        <rdfs:label xml:lang="en">mentioned</rdfs:label>
        <scopeNote xml:lang="en">A person or institution that is explicitly mentioned in the (textual) CHO.</scopeNote>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/misattributed -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/misattributed">
        <rdfs:label xml:lang="en">misattributed</rdfs:label>
        <scopeNote xml:lang="en">A person or institution that is mentioned as the creator of the CHO although he or it is not the creator.</scopeNote>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    



    <!-- http://onto.dm2e.eu/schemas/dm2e/owner -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/owner">
        <rdfs:label xml:lang="en">owner</rdfs:label>
        <scopeNote xml:lang="en">Indicates the ownership of a CHO.</scopeNote>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/painter -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/painter">
        <rdfs:label xml:lang="en">painter</rdfs:label>
        <scopeNote xml:lang="en">A painter. In the manuscript context especially used in works of the 16th and 17th century (rare books).</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;contributor"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
