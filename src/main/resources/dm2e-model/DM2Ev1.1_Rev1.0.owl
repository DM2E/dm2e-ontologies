        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;Place"/>
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;TimeSpan"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/hasVersion -->

    <owl:ObjectProperty rdf:about="&dcterms;hasVersion">
