    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/modified -->

    <owl:ObjectProperty rdf:about="&dcterms;modified">
        <scopeNote xml:lang="eng">The property dcterms:created holds the modification date and time of the aggregation, i.e. the original source metadata record.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;date"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/references -->

    <owl:ObjectProperty rdf:about="&dcterms;references">
        <scopeNote xml:lang="eng">Another CHO referenced in the content of this CHO.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/rightsHolder -->

    <owl:ObjectProperty rdf:about="&dcterms;rightsHolder">
        <scopeNote xml:lang="eng">The property dcterms:rightsHolder holds the name or identifier of the agent (a person or organisation) who owns or manages the rights of the physical object (the CHO) which is described in the source metadata record.</scopeNote>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
