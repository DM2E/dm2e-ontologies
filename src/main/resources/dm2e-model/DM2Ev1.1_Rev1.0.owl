        <scopeNote xml:lang="eng">The dataset that contains the RDF statements provided via this data resource.</scopeNote>
        <rdfs:domain rdf:resource="http://onto.dm2e.eu/schemas/dm2e/1.1/DataResource"/>
        <rdfs:range rdf:resource="&void;Dataset"/>
    </owl:ObjectProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfEstablishment -->

    <owl:ObjectProperty rdf:about="&rdaGr2;dateOfEstablishment">
        <scopeNote xml:lang="eng">The date the institution was established.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dc;date"/>
        <rdfs:domain rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://rdvocab.info/ElementsGr2/dateOfTermination -->

    <owl:ObjectProperty rdf:about="&rdaGr2;dateOfTermination">
