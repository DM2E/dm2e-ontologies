    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/artist -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/artist">
        <rdfs:label xml:lang="en">artist</rdfs:label>
        <rdfs:comment xml:lang="eng">An artist that has created (e.g. painted) the CHO.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;creator"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/composer -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/composer">
        <rdfs:label xml:lang="en">composer</rdfs:label>
        <rdfs:comment xml:lang="eng">The person that has composed a CHO (e.g. a letter). Can be the same as the person who wrote the letter, but can also be a different person, e.g. someone who has dictated but not wrote the letter.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;creator"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/contributor -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/contributor">
        <rdfs:label xml:lang="en">contributor</rdfs:label>
        <rdfs:comment xml:lang="eng">A person that is responsible for making contributions to the resource.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;contributor"/>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/copyist -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/copyist">
        <rdfs:label xml:lang="en">copyist</rdfs:label>
        <rdfs:comment xml:lang="eng">Someone who copied a CHO.</rdfs:comment>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/hasAnnotatableVersionAt -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/hasAnnotatableVersionAt">
        <rdfs:comment xml:lang="eng">The property dm2e:hasAnnotatableVersionAt holds an URL which leads to an HTML representation or to an image of the content, i.e. of the CHO aggregated by the current Aggregation. The URL of the HTML content or image file must be stable. The type of the WebResource must be further specified as ‚Äútext/html-named-content‚Äù, ‚Äútext/plain‚Äù, ‚Äúimage/gif‚Äù, ‚Äúimage/jpeg‚Äù or ‚Äúimage/png‚Äù.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/honoree -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/honoree">
        <rdfs:label xml:lang="en">honoree</rdfs:label>
        <rdfs:comment xml:lang="eng">An honoured person, for whom the CHO is published or created.</rdfs:comment>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/influencedBy -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/influencedBy">
        <rdfs:label xml:lang="en">influenced by</rdfs:label>
        <rdfs:comment xml:lang="eng">Another person by which the person was influenced.</rdfs:comment>
        <rdfs:domain rdf:resource="&foaf;Person"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/mentioned -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/mentioned">
        <rdfs:label xml:lang="en">mentioned</rdfs:label>
        <rdfs:comment xml:lang="eng">A person or institution that is explicitly mentioned in the (textual) CHO.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/misattributed -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/misattributed">
        <rdfs:label xml:lang="en">misattributed</rdfs:label>
        <rdfs:comment xml:lang="eng">A person or institution that is mentioned as the creator of the CHO although he or it is not the creator.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/owner -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/owner">
        <rdfs:label xml:lang="en">owner</rdfs:label>
        <rdfs:comment xml:lang="eng">Indicates the ownership of a CHO.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/painter -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/painter">
        <rdfs:label xml:lang="en">painter</rdfs:label>
        <rdfs:comment xml:lang="eng">A painter. In the manuscript context especially used in works of the 16th and 17th century (rare books).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dc;creator"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/patron -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/patron">
        <rdfs:label xml:lang="en">patron</rdfs:label>
        <rdfs:comment xml:lang="eng">A person who is honored within the text.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/1.1/contributor"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/portrayed -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/portrayed">
        <rdfs:label xml:lang="en">portrayed</rdfs:label>
        <rdfs:comment xml:lang="eng">A person that is portrayed in the (textual or painted) CHO by its creator.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://onto.dm2e.eu/schemas/dm2e/1.1/mentioned"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/previousOwner -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/previousOwner">
        <rdfs:label xml:lang="en">previous owner</rdfs:label>
        <rdfs:comment xml:lang="eng">A person or an institution that has owned the CHO before.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/principal -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/principal">
        <rdfs:label xml:lang="en">principal</rdfs:label>
        <rdfs:comment xml:lang="eng">A person or an institution that gave the order to create the CHO.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/printedAt -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/printedAt">
        <rdfs:label xml:lang="en">printed at</rdfs:label>
        <rdfs:comment xml:lang="eng">Indicates the place where the CHO was printed.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;spatial"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/publishedAt -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/publishedAt">
        <rdfs:label xml:lang="en">published at</rdfs:label>
        <rdfs:comment xml:lang="eng">The place of a physical publication or the web resource of a web publication.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;coverage"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;Place"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/refersTo -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/refersTo">
        <rdfs:label xml:lang="en">refers to</rdfs:label>
        <rdfs:comment xml:lang="eng">A person or CHO that is explicitly or implicitly menioned in the (textual) CHO.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&dcterms;references"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&foaf;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/shelfmarkLocation -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/shelfmarkLocation">
        <rdfs:label xml:lang="en">shelfmark location</rdfs:label>
        <rdfs:comment xml:lang="eng">Shelfmark location from the CHO.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&edm;currentLocation"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/sponsor -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/sponsor">
        <rdfs:label xml:lang="en">sponsor</rdfs:label>
        <rdfs:comment xml:lang="eng">A person or an institution who has sponsored (parts) of the CHOs creation.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/studentOf -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/studentOf">
        <rdfs:label xml:lang="en">student of</rdfs:label>
        <rdfs:comment xml:lang="eng">A teacher of the person.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/wasStudiedBy -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/wasStudiedBy">
        <rdfs:label xml:lang="en">was studied by</rdfs:label>
        <rdfs:comment xml:lang="eng">The CHO was studied by an Agent.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/wasTaughtBy -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/wasTaughtBy">
        <rdfs:label xml:lang="en">was taught by</rdfs:label>
        <rdfs:comment xml:lang="eng">The CHO was taught by an Agent.</rdfs:comment>
        <rdfs:range rdf:resource="&edm;Agent"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasMet"/>
    </owl:ObjectProperty>
    


    <!-- http://onto.dm2e.eu/schemas/dm2e/1.1/writer -->

    <owl:ObjectProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/writer">
        <rdfs:label xml:lang="en">writer</rdfs:label>
        <rdfs:comment xml:lang="eng">A person who has written a CHO, e.g. a letter.</rdfs:comment>
