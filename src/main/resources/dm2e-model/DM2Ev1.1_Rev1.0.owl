    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/isPartOf -->

    <owl:ObjectProperty rdf:about="&dcterms;isPartOf">
        <scopeNote xml:lang="eng">dcterms:isPartOf can be used to indicate that a class is part of the current class. It can only be used for the same type of objects, e.g. edm:ProvidedCHO dcterms:isPartOf edm:ProvidedCHO.</scopeNote>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;Place"/>
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;TimeSpan"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;Place"/>
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;TimeSpan"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/isVersionOf -->

    <owl:ObjectProperty rdf:about="&dcterms;isVersionOf">
