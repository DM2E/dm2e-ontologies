        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/bibo/editor -->

