        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isNextInSenquence -->

    <owl:ObjectProperty rdf:about="&edm;isNextInSenquence">
        <scopeNote xml:lang="eng">Preceding same-level CHO, e.g. previous chapter.</scopeNote>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;ProvidedCHO"/>
                    <rdf:Description rdf:about="&edm;WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isShownAt -->

    <owl:ObjectProperty rdf:about="&edm;isShownAt">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="eng">The property edm:isShownAt holds the URL of a edm:WebResource which leads to a view of the digital object on the provider&#39;s web site in its full information context (e.g. in a viewer application). Either edm:isShownBy or edm:isShownAt must be provided.</scopeNote>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isShownBy -->

    <owl:ObjectProperty rdf:about="&edm;isShownBy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <scopeNote xml:lang="eng">The property edm:isShownBy holds the URL of a edm:WebResource which leads to a &quot;plain&quot; image with any kind of view of the edm:ProvidedCHO. &quot;Plain&quot; image means that the image must be without any information context, for example, the URL points to a plain JPG-image. Either edm:isShownBy or edm:isShownAt must be provided.</scopeNote>
        <rdfs:range rdf:resource="&edm;WebResource"/>
        <rdfs:subPropertyOf rdf:resource="&edm;hasView"/>
        <rdfs:domain rdf:resource="&ore;Aggregation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isSimilarTo -->

    <owl:ObjectProperty rdf:about="&edm;isSimilarTo"/>
    


    <!-- http://www.europeana.eu/schemas/edm/landingPage -->

