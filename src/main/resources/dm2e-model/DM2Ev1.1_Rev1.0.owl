    <!-- http://purl.org/dc/elements/1.1/type -->

    <owl:ObjectProperty rdf:about="&dc;type">
        <scopeNote xml:lang="eng">The property dc:type provides a specific type that applies to the CHO. By type, we mean either a physical form (e.g. book) or logical form (e.g. paragraph) as they are specified as subclasses of edm:PhysicalThing and skos:Concept respectively. At least one dc:type must be provided.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&edm;PhysicalThing"/>
                    <rdf:Description rdf:about="&skos;Concept"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/coverage -->

    <owl:ObjectProperty rdf:about="&dcterms;coverage">
