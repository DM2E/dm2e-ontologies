                    <rdf:Description rdf:about="&ore;Aggregation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/coverage -->

    <owl:ObjectProperty rdf:about="&dc;coverage"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:ObjectProperty rdf:about="&dc;creator">
        <scopeNote xml:lang="eng">The property dcterms:creator holds the name or identifier of the agent (a person or organisation) who created the aggregation, i.e. the original source metadata record.</scopeNote>
