        <rdfs:domain rdf:resource="&ore;Aggregation"/>
        <rdfs:subPropertyOf rdf:resource="&ore;aggregates"/>
    </owl:ObjectProperty>
    


    <!-- http://www.europeana.eu/schemas/edm/isDerivativeOf -->

    <owl:ObjectProperty rdf:about="&edm;isDerivativeOf">
        <scopeNote xml:lang="eng">Original version (in case of CHOs) or a related resource (in case of Webresources) from which this object has been derived in whole or in part.</scopeNote>
