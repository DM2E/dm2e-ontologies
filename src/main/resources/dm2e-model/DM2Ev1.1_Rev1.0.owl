                    <rdf:Description rdf:about="&ore;Aggregation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/spatial -->

    <owl:ObjectProperty rdf:about="&dcterms;spatial">
        <scopeNote xml:lang="eng">This property can be used for not further specified places. If the place of publication or printing should be indicated, use dm2e:publishedAt or dme2e:printedAt instead.</scopeNote>
        <rdfs:subPropertyOf rdf:resource="&dcterms;coverage"/>
        <rdfs:range rdf:resource="&edm;Place"/>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/tableOfContents -->

    <owl:ObjectProperty rdf:about="&dcterms;tableOfContents">
