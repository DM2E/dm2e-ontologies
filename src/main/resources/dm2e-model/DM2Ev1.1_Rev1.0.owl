    <rdf:Description rdf:about="&dc;creator">
        <scopeNote xml:lang="eng">The property dcterms:creator holds the name or identifier of the agent (a person or organisation) who created the aggregation, i.e. the original source metadata record.</scopeNote>
    </rdf:Description>
    <owl:AnnotationProperty rdf:about="http://onto.dm2e.eu/schemas/dm2e/1.1/scopeNote">
        <rdfs:comment xml:lang="eng">This property describes the dm2e-specific usage of the resource.</rdfs:comment>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&dcterms;alternative">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <scopeNote xml:lang="eng">An alternative name for the resource. Should not be used for subtitles in DM2E. Can be used if title and subtitle are in one field. A language tag in ISO 639-2 should be added to the Literal.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&vann;preferredNamespaceUri"/>
    <owl:AnnotationProperty rdf:about="&skos;prefLabel">
        <scopeNote xml:lang="eng">The preferred name of the agent, preferably in a normalized form. Only one preferred label per language tag is allowed!</scopeNote>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&foaf;homepage"/>
    <owl:AnnotationProperty rdf:about="&adms;relatedWebPage"/>
    <owl:AnnotationProperty rdf:about="&skos;hiddenLabel">
        <scopeNote xml:lang="eng">Non-standard forms of the name, e.g. misspellings.</scopeNote>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&vann;example"/>
    <owl:AnnotationProperty rdf:about="&radion;versionNotes"/>
    <owl:AnnotationProperty rdf:about="&skos;note">
        <scopeNote xml:lang="eng">Information related to the resource that cannot be modelled with other properties of the class.</scopeNote>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&dc;modified"/>
    <owl:AnnotationProperty rdf:about="&vann;preferredNamespacePrefix"/>
    <owl:AnnotationProperty rdf:about="&vann;changes"/>
    <owl:AnnotationProperty rdf:about="&dcterms;issued">
        <scopeNote xml:lang="eng">Date of publication (of the described CHO, usually the original one).</scopeNote>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&skos;definition"/>
    <owl:AnnotationProperty rdf:about="&skos;altLabel">
        <scopeNote xml:lang="eng">An alternative name, e.g. a former name or the name in another form.</scopeNote>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&dc;title">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <scopeNote xml:lang="eng">The property dc:titles gives the main title of the CHO. It is mandatory to provide either a dc:title or a dc:description for the CHO. A language tag in ISO 639-2 should be added to the Literal.</scopeNote>
        <rdfs:domain rdf:resource="&edm;ProvidedCHO"/>
    </owl:AnnotationProperty>
    


